  <owl:ObjectProperty rdf:about="http://purl.org/obo/owl/obo#inheres_in">
    <rdfs:label xml:lang="en">inheres_in</rdfs:label>
    <rdfs:comment>Examples: the redness inhering in an eye of a wild-type fruitfly</rdfs:comment>
    <oboInOwl:hasDefinition>
      <oboInOwl:Definition>
        <rdfs:label xml:lang="en">A relation between a dependent continuant and an entity. D inheres_in E iff: any any dDt, exists eEt such that d inheres_in e at t</rdfs:label>
      </oboInOwl:Definition>
    </oboInOwl:hasDefinition>
    <oboInOwl:hasOBONamespace>quality</oboInOwl:hasOBONamespace>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/obo/owl/obo#towards">
    <rdfs:label xml:lang="en">towards</rdfs:label>
    <rdfs:comment>Examples: sensitivity towards chlorine.</rdfs:comment>
    <oboInOwl:hasDefinition>
      <oboInOwl:Definition>
        <rdfs:label xml:lang="en">A relation between a relational quality and an entity type. Q towards E iff: for any qQt, q towards E at t.</rdfs:label>
      </oboInOwl:Definition>
    </oboInOwl:hasDefinition>
    <oboInOwl:hasRelatedSynonym>
      <oboInOwl:Synonym>
        <rdfs:label xml:lang="en">with respect to</rdfs:label>
      </oboInOwl:Synonym>
    </oboInOwl:hasRelatedSynonym>
    <oboInOwl:hasRelatedSynonym>
      <oboInOwl:Synonym>
        <rdfs:label xml:lang="en">directed towards</rdfs:label>
      </oboInOwl:Synonym>
    </oboInOwl:hasRelatedSynonym>
    <oboInOwl:hasRelatedSynonym>
      <oboInOwl:Synonym>
        <rdfs:label xml:lang="en">depends_on</rdfs:label>
      </oboInOwl:Synonym>
    </oboInOwl:hasRelatedSynonym>
